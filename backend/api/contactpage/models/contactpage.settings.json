{
  "kind": "singleType",
  "collectionName": "contactpages",
  "info": {
    "name": "Contactpage"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
